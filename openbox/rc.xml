    <!-- Misc integration -->
    <keybind key='W-S-d'><action name='Execute'>
      <command>toggle-process conky</command></action></keybind>

    <keybind key='W-n'><action name='Execute'>
      <command>synclient TouchpadOff=1</command></action></keybind>
    <keybind key='W-S-n'><action name='Execute'>
      <command>synclient TouchpadOff=0</command></action></keybind>

    <!-- Audio -->
    <keybind key='W-Down'>
      <action name='Execute'><command>vol-delta -5</command></action></keybind>
    <keybind key='W-Up'>
      <action name='Execute'><command>vol-delta 5</command></action></keybind>
    <keybind key='W-S-Left'><action name='Execute'>
      <command>vol-absolute 0</command></action></keybind>
    <keybind key='W-S-Right'><action name='Execute'>
      <command>vol-absolute 56</command></action></keybind>
    <keybind key='W-Left'><action name='Execute'>
      <command>vol-absolute 24</command></action></keybind>
    <keybind key='W-Right'><action name='Execute'>
      <command>vol-absolute 41</command></action></keybind>

