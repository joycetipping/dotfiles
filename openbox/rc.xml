    <keybind key="A-space"><action name="ShowMenu">
      <menu>client-menu</menu></action></keybind>

    <!-- Keybindings for window switching -->
    <keybind key="A-Tab"><action name="NextWindow"/></keybind>
    <keybind key="A-S-Tab"><action name="NextWindow">
      <allDesktops>yes</allDesktops></action></keybind>

    <keybind key="W-space">
      <action name="ShowMenu"><menu>root-menu</menu></action></keybind>
